<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.star.mall.mapper.CategoryMapper">
    <insert id="categoryAdded" parameterType="com.star.mall.pojo.Category">
        insert into category
        values (cateId, name, descn)
    </insert>
    <select id="categoryQueryList" resultType="com.star.mall.pojo.Category">
        select *
        from category
    </select>
</mapper>